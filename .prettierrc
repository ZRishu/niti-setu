{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,

  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",

  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,

  "arrowParens": "always",
  "endOfLine": "lf",

  "jsxSingleQuote": false,
  "jsxBracketSameLine": false,

  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",

  "embeddedLanguageFormatting": "auto"
}
